    "\"\"\"for the given string create a new string with each original letter is repeated 3 times\"\"\"\n",
    "def repeat3Times(string):\n",
    "    if not isinstance(string,str):\n",
    "        return 'invalid input'\n",
    "    newString = ''\n",
    "    for i in string:\n",
    "        if not i==' ':\n",
    "            newString += i*3\n",
    "        else:\n",
    "            newString += i\n",
    "    return newString"
   ]
