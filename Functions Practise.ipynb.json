    "def myfunc(*args):\n",
    "    #function returns lowest number of the list if it contains any even number in it. If all numbers then return a large number\n",
    "    #we can also use sorting of list and using list[0] and list [-1] example, argString = sorted(args),return list[0] for min, return list[-1] for maximum\n",
    "    print(args)\n",
    "    even = False\n",
    "    for i in args:\n",
    "        if (i%2 == 0):\n",
    "            even = True\n",
    "            return min(args)\n",
    "    \n",
    "    if not even:\n",
    "        return max(args)\n",
    "    "
   ]
