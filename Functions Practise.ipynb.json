    "\"\"\"In Given list if 3 appears next to another 3 return true\"\"\"\n",
    "def check_3(lists):\n",
    "    FirstNumber_3 = False\n",
    "    if not isinstance(lists,list):\n",
    "        return 'invalid input'\n",
    "    for i in lists:\n",
    "        if FirstNumber_3 and (int(i) == 3):\n",
    "            return True\n",
    "        else:\n",
    "            FirstNumber_3 = False\n",
    "        if int(i) == 3:\n",
    "            FirstNumber_3 = True\n",
    "    else:\n",
    "        return 'No consecutive 3\\'s in list'\n",
    "def pythonicWay_check_3(lists):\n",
    "    if not isinstance(lists,list):\n",
    "        return 'invalid input'\n",
    "    for i in range(0,len(lists)-1):\n",
    "        if lists[i:i+2]== [3,3]:\n",
    "            return True\n",
    "    else:\n",
    "        return 'No consecutive 3\\'s in list'"
