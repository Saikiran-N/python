    "def summer_69(*args):\n",
    "    lists = list(args)\n",
    "    print(f\"Input provided {lists}\")\n",
    "    skipVal = False\n",
    "    firstskip = 0\n",
    "    Total = 0\n",
    "    \n",
    "    for i in range(0,len(lists)):\n",
    "        if lists[i] == 6:\n",
    "            firstskip = 1\n",
    "            continue\n",
    "        if firstskip and lists[i]==9:\n",
    "            firstskip = 2\n",
    "            break\n",
